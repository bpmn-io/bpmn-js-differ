{
  "name": "bpmn-js-differ",
  "version": "2.0.2",
  "description": "A semantic diffing utility for BPMN 2.0 files",
  "scripts": {
    "all": "run-s lint test",
    "lint": "eslint .",
    "pretest": "rollup -c --bundleConfigAsCjs",
    "test": "mocha test/spec/*.js",
    "prepublishOnly": "rollup -c --bundleConfigAsCjs"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.esm.js",
      "node": "./dist/index.cjs.js"
    }
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/bpmn-js-differ"
  },
  "keywords": [
    "bpmnjs",
    "diff"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/Nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "bpmn-moddle": "^8.1.0",
    "chai": "^4.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-bpmn-io": "^1.0.1",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.2",
    "rollup": "^4.19.0"
  },
  "dependencies": {
    "diffpatch": "^0.6.0",
    "min-dash": "^4.2.1"
  },
  "files": [
    "dist"
  ]
}
